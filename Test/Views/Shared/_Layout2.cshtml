    <script src="~/depan/js/bootstrap.min.js"></script>
    <script src="~/depan/js/jquery.isotope.min.js"></script>
    <script src="~/depan/js/jquery.nicescroll.min.js"></script>
    <script src="~/depan/js/fancybox/jquery.fancybox.pack.js"></script>
    <script src="~/depan/js/skrollr.min.js"></script>
    <script src="~/depan/js/jquery.scrollTo.js"></script>
